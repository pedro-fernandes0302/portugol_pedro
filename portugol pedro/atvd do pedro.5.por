programa {
  funcao inicio() {
    real km
    escreva("qual sua distancia em km?:")
    leia (km)
      se (km<100)
      escreva("o frete a ser pago é de :R$ 10")
        se (km>100 ou km<300)
        escreva ("o frete a ser pago é de:R$ 20")
          se (km>=300)
          escreva("o frete a ser pago é de:R$ 30")

  }
}
